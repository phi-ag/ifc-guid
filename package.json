{
  "type": "module",
  "name": "@phi-ag/ifc-guid",
  "version": "1.1.2",
  "license": "MIT",
  "description": "Convert IFC GUID",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-ag/ifc-guid.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ifc",
    "uuid",
    "guid"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc --build",
    "typecheck": "tsc --noEmit",
